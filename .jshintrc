  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "maxlen": 100,
  "eqnull": true,
  "esnext": true,
  "expr": true,
  "proto": true,
  "node": true,
  "predef": [
    "describe",
